// Simple fizzbuzz example in Unilang

@include "stdlib/io.ul" 

let fizzbuzz(n: i32): void => {
  loop i: 1 ->> n => {
    if(i > 1) => print(", ");
    if(i % 3 == 0 && i % 5 == 0) => print("Fizz Buzz");
    else if(i % 3 == 0) => print("Fizz");
    else if(i % 5 == 0) => print("Buzz");
    else print_num(i);
  }
  println("");
}
 
let entry(): void => {
  fizzbuzz(36);
}